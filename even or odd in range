#include <stdio.h>
int main() 
{
    int numbers[] = {1, 2, 3, 4, 5};
    int sum = 0;
    for (int i = 0; i < 5; i++)
{
        sum += numbers[i];
    }
    if (sum % 2 == 0) 
{
        printf("The sum of the numbers is even.\n");
    } 
else 
{
        printf("The sum of the numbers is odd.\n");
    }
    return 0;
}
